{
  "dashboard": {
    "id": null,
    "title": "Fragrance AI - Production Monitoring Dashboard",
    "tags": ["fragrance-ai", "monitoring", "production"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            }
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "targets": [
          {
            "expr": "up{job=\"fragrance-ai\"}",
            "refId": "A",
            "legendFormat": "Service Status"
          },
          {
            "expr": "rate(http_requests_total{job=\"fragrance-ai\"}[5m])",
            "refId": "B",
            "legendFormat": "Requests/sec"
          },
          {
            "expr": "process_resident_memory_bytes{job=\"fragrance-ai\"} / 1024 / 1024",
            "refId": "C",
            "legendFormat": "Memory Usage (MB)"
          },
          {
            "expr": "rate(process_cpu_seconds_total{job=\"fragrance-ai\"}[5m]) * 100",
            "refId": "D",
            "legendFormat": "CPU Usage (%)"
          }
        ]
      },
      {
        "id": 2,
        "title": "API Performance Metrics",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "unit": "ms"
          }
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single"
          }
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"fragrance-ai\"}[5m])) * 1000",
            "refId": "A",
            "legendFormat": "p50 Response Time"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"fragrance-ai\"}[5m])) * 1000",
            "refId": "B",
            "legendFormat": "p95 Response Time"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"fragrance-ai\"}[5m])) * 1000",
            "refId": "C",
            "legendFormat": "p99 Response Time"
          }
        ]
      },
      {
        "id": 3,
        "title": "Error Rate by Endpoint",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "percent"
          }
        },
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"fragrance-ai\", status=~\"5..\"}[5m]) / rate(http_requests_total{job=\"fragrance-ai\"}[5m]) * 100",
            "refId": "A",
            "legendFormat": "5xx Error Rate by {{method}} {{handler}}"
          },
          {
            "expr": "rate(http_requests_total{job=\"fragrance-ai\", status=~\"4..\"}[5m]) / rate(http_requests_total{job=\"fragrance-ai\"}[5m]) * 100",
            "refId": "B",
            "legendFormat": "4xx Error Rate by {{method}} {{handler}}"
          }
        ]
      },
      {
        "id": 4,
        "title": "AI Model Performance",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 17},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "unit": "ms"
          }
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(fragrance_ai_model_inference_duration_seconds_bucket{model_type=\"embedding\"}[5m])) * 1000",
            "refId": "A",
            "legendFormat": "Embedding Model p95 Latency"
          },
          {
            "expr": "histogram_quantile(0.95, rate(fragrance_ai_model_inference_duration_seconds_bucket{model_type=\"generation\"}[5m])) * 1000",
            "refId": "B",
            "legendFormat": "Generation Model p95 Latency"
          },
          {
            "expr": "histogram_quantile(0.95, rate(fragrance_ai_rag_generation_duration_seconds_bucket[5m])) * 1000",
            "refId": "C",
            "legendFormat": "RAG Generation p95 Latency"
          }
        ]
      },
      {
        "id": 5,
        "title": "Cache Performance",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 17},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "unit": "percent"
          }
        },
        "targets": [
          {
            "expr": "rate(fragrance_ai_cache_hits_total[5m]) / (rate(fragrance_ai_cache_hits_total[5m]) + rate(fragrance_ai_cache_misses_total[5m])) * 100",
            "refId": "A",
            "legendFormat": "Cache Hit Rate"
          },
          {
            "expr": "fragrance_ai_cache_size_bytes / fragrance_ai_cache_max_size_bytes * 100",
            "refId": "B",
            "legendFormat": "Cache Usage"
          }
        ]
      },
      {
        "id": 6,
        "title": "Database Performance",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 26},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "unit": "ms"
          }
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(fragrance_ai_db_query_duration_seconds_bucket[5m])) * 1000",
            "refId": "A",
            "legendFormat": "Database Query p95 Latency"
          },
          {
            "expr": "rate(fragrance_ai_db_connections_active[5m])",
            "refId": "B",
            "legendFormat": "Active DB Connections"
          }
        ]
      },
      {
        "id": 7,
        "title": "Security Alerts & Authentication",
        "type": "table",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 26},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            }
          }
        },
        "options": {
          "showHeader": true
        },
        "targets": [
          {
            "expr": "rate(fragrance_ai_auth_failures_total[5m])",
            "refId": "A",
            "legendFormat": "Authentication Failures/sec"
          },
          {
            "expr": "rate(fragrance_ai_security_violations_total[5m])",
            "refId": "B",
            "legendFormat": "Security Violations/sec"
          },
          {
            "expr": "fragrance_ai_active_sessions",
            "refId": "C",
            "legendFormat": "Active User Sessions"
          }
        ]
      },
      {
        "id": 8,
        "title": "Real-time System Metrics",
        "type": "stat",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 35},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "mappings": [
              {
                "options": {
                  "0": {"text": "DOWN", "color": "red"},
                  "1": {"text": "UP", "color": "green"}
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            }
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "targets": [
          {
            "expr": "up{job=\"fragrance-ai-embedding\"}",
            "refId": "A",
            "legendFormat": "Embedding Service"
          },
          {
            "expr": "up{job=\"fragrance-ai-generation\"}",
            "refId": "B",
            "legendFormat": "Generation Service"
          },
          {
            "expr": "up{job=\"fragrance-ai-database\"}",
            "refId": "C",
            "legendFormat": "Database"
          },
          {
            "expr": "up{job=\"fragrance-ai-redis\"}",
            "refId": "D",
            "legendFormat": "Redis Cache"
          },
          {
            "expr": "up{job=\"fragrance-ai-vector-db\"}",
            "refId": "E",
            "legendFormat": "Vector Database"
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "links": [],
    "liveNow": false,
    "panels": [],
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "prometheus",
          "definition": "label_values(up{job=~\"fragrance-ai.*\"}, instance)",
          "hide": 0,
          "includeAll": true,
          "label": "Instance",
          "multi": true,
          "name": "instance",
          "options": [],
          "query": {
            "query": "label_values(up{job=~\"fragrance-ai.*\"}, instance)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "tagValuesQuery": "",
          "tagsQuery": "",
          "type": "query",
          "useTags": false
        }
      ]
    }
  }
