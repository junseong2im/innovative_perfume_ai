{
  "dashboard": {
    "title": "LLM Ensemble Monitoring",
    "tags": ["llm", "ai", "fragrance"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Rate (RPS)",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(llm_requests_total[1m])",
            "legendFormat": "{{model}} - {{mode}} - {{status}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Active Requests",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "llm_active_requests",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Inference Latency (p50, p95, p99)",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 8, "w": 24, "h": 9},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(llm_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "p50 - {{model}} - {{mode}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(llm_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{model}} - {{mode}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(llm_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "p99 - {{model}} - {{mode}}",
            "refId": "C"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "last"]
          },
          "tooltip": {
            "mode": "multi"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "lineWidth": 2
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*p95.*"},
              "properties": [
                {
                  "id": "custom.lineStyle",
                  "value": {"fill": "dash"}
                }
              ]
            },
            {
              "matcher": {"id": "byRegexp", "options": ".*p99.*"},
              "properties": [
                {
                  "id": "custom.lineStyle",
                  "value": {"fill": "dot"}
                }
              ]
            }
          ]
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 17, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(llm_errors_total[1m])",
            "legendFormat": "{{model}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "errors/s",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "fillOpacity": 20
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Cache Hit Rate",
        "type": "stat",
        "gridPos": {"x": 12, "y": 17, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "rate(llm_cache_hits_total[5m]) / (rate(llm_cache_hits_total[5m]) + rate(llm_cache_misses_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Total Requests (Success vs Error)",
        "type": "stat",
        "gridPos": {"x": 18, "y": 17, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(llm_requests_total{status=\"success\"}[5m]))",
            "legendFormat": "Success",
            "refId": "A"
          },
          {
            "expr": "sum(rate(llm_requests_total{status=\"error\"}[5m]))",
            "legendFormat": "Error",
            "refId": "B"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Success"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Error"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
              ]
            }
          ]
        }
      },
      {
        "id": 7,
        "title": "Model Status (Loaded)",
        "type": "stat",
        "gridPos": {"x": 0, "y": 25, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "llm_model_loaded",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "mappings": [
              {"type": "value", "value": 0, "text": "Not Loaded"},
              {"type": "value", "value": 1, "text": "Loaded"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "RAM Memory Usage",
        "type": "timeseries",
        "gridPos": {"x": 8, "y": 25, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "llm_model_memory_bytes{memory_type=\"ram\"}",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Request Distribution by Model",
        "type": "piechart",
        "gridPos": {"x": 16, "y": 25, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "sum by (model) (rate(llm_requests_total[5m]))",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "pieType": "donut",
          "tooltip": {
            "mode": "single"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 10,
        "title": "Cache Operations",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 33, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(llm_cache_hits_total[1m])",
            "legendFormat": "Hits",
            "refId": "A"
          },
          {
            "expr": "rate(llm_cache_misses_total[1m])",
            "legendFormat": "Misses",
            "refId": "B"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Hits"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Misses"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}
              ]
            }
          ]
        }
      },
      {
        "id": 11,
        "title": "Average Latency by Mode",
        "type": "bargauge",
        "gridPos": {"x": 12, "y": 33, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "avg by (mode) (rate(llm_inference_duration_seconds_sum[5m]) / rate(llm_inference_duration_seconds_count[5m]))",
            "legendFormat": "{{mode}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1.0, "color": "yellow"},
                {"value": 2.5, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Error Types Distribution",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 41, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "sum by (error_type) (rate(llm_errors_total[5m]))",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "last"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "errors/s",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "fillOpacity": 20,
              "stacking": {
                "mode": "normal"
              }
            }
          }
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(llm_model_loaded[5m]) > 0",
          "iconColor": "blue",
          "name": "Model Reload",
          "step": "1m",
          "tagKeys": "model",
          "textFormat": "Model {{model}} reloaded",
          "titleFormat": "Model Reload"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "text": "Prometheus",
            "value": "Prometheus"
          }
        },
        {
          "name": "model",
          "type": "query",
          "datasource": "$datasource",
          "query": "label_values(llm_requests_total, model)",
          "multi": true,
          "includeAll": true,
          "current": {
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "mode",
          "type": "query",
          "datasource": "$datasource",
          "query": "label_values(llm_requests_total, mode)",
          "multi": true,
          "includeAll": true,
          "current": {
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    }
  },
  "overwrite": true
}
