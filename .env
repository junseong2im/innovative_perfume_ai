# Fragrance AI 개발 환경 설정
# 개발용 설정 - 프로덕션에서는 반드시 값들을 변경하세요

# ==============================================================================
# 애플리케이션 설정
# ==============================================================================
ENVIRONMENT=development
SECRET_KEY=development-secret-key-change-this-in-production-12345678901234567890
DEBUG=true

# ==============================================================================
# 데이터베이스 설정
# ==============================================================================
# PostgreSQL
DATABASE_URL=postgresql://fragrance_ai:fragrance_ai_password@localhost:5432/fragrance_ai
POSTGRES_PASSWORD=fragrance_ai_password

# Redis
REDIS_URL=redis://:redis_password@localhost:6379/0
REDIS_PASSWORD=redis_password

# ChromaDB
CHROMA_HOST=localhost
CHROMA_PORT=8000
CHROMA_AUTH_TOKEN=dev_chroma_token

# ==============================================================================
# AI 모델 설정
# ==============================================================================
# 임베딩 모델
EMBEDDING_MODEL_NAME=sentence-transformers/paraphrase-multilingual-MiniLM-L12-v2
EMBEDDING_DEVICE=cpu
EMBEDDING_BATCH_SIZE=32

# 생성 모델
GENERATION_MODEL_NAME=microsoft/DialoGPT-medium
GENERATION_DEVICE=cpu
USE_4BIT_QUANTIZATION=false
USE_8BIT_QUANTIZATION=false

# ==============================================================================
# API 설정
# ==============================================================================
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=1
FRONTEND_URL=http://localhost:3000

# CORS 설정
CORS_ORIGINS=["http://localhost:3000", "http://127.0.0.1:3000"]

# ==============================================================================
# 보안 설정
# ==============================================================================
# JWT 토큰
JWT_SECRET_KEY=jwt-secret-key-for-development-only
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60

# API 키 (개발용)
API_KEY_REQUIRED=false
ADMIN_API_KEY=dev-admin-key

# Rate Limiting
RATE_LIMIT_ENABLED=false
REQUESTS_PER_MINUTE=1000
REQUESTS_PER_HOUR=50000

# ==============================================================================
# 스토리지 및 파일 경로
# ==============================================================================
# 모델 저장 경로
MODEL_CACHE_DIR=./models
CHECKPOINT_DIR=./models/checkpoints

# 데이터 경로
DATA_DIR=./data
TRAINING_DATA_DIR=./data/training
EVALUATION_DATA_DIR=./data/evaluation

# 로그 경로
LOG_DIR=./logs
LOG_LEVEL=DEBUG
STRUCTURED_LOGGING=false

# ==============================================================================
# 외부 서비스 (개발용 - 비활성화)
# ==============================================================================
# Weights & Biases
WANDB_ENABLED=false
WANDB_API_KEY=
WANDB_PROJECT=fragrance-ai-dev
WANDB_ENTITY=

# MLflow
MLFLOW_TRACKING_URI=
MLFLOW_EXPERIMENT_NAME=fragrance-ai-dev

# ==============================================================================
# 모니터링 및 알림 (개발용 - 비활성화)
# ==============================================================================
# Prometheus
PROMETHEUS_ENABLED=false
METRICS_PORT=9090

# 알림
ALERT_WEBHOOK_URL=
SLACK_CHANNEL=
ALERT_EMAIL=

# ==============================================================================
# Celery 설정
# ==============================================================================
CELERY_BROKER_URL=redis://:redis_password@localhost:6379/1
CELERY_RESULT_BACKEND=redis://:redis_password@localhost:6379/2
CELERY_TASK_SERIALIZER=json
CELERY_ACCEPT_CONTENT=["json"]
CELERY_RESULT_SERIALIZER=json
CELERY_TIMEZONE=UTC

# ==============================================================================
# 백업 설정 (개발용 - 비활성화)
# ==============================================================================
BACKUP_ENABLED=false
BACKUP_S3_BUCKET=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
BACKUP_RETENTION_DAYS=7

# ==============================================================================
# SSL/TLS 설정 (개발용 - HTTP)
# ==============================================================================
HTTPS_ENABLED=false
SSL_CERT_PATH=
SSL_KEY_PATH=
LETSENCRYPT_EMAIL=
DOMAIN_NAME=

# ==============================================================================
# 성능 및 스케일링 설정
# ==============================================================================
# 캐시 설정
CACHE_TTL_SECONDS=3600
SEARCH_CACHE_TTL=1800
GENERATION_CACHE_TTL=3600
MAX_CACHE_SIZE=1000

# 요청 제한
MAX_REQUEST_SIZE=10485760
REQUEST_TIMEOUT=300

# 워커 설정
GUNICORN_WORKERS=1
CELERY_WORKER_CONCURRENCY=2
CELERY_WORKER_PREFETCH_MULTIPLIER=1

# ==============================================================================
# 개발/디버깅 설정
# ==============================================================================
# 개발 환경 전용
ENABLE_DEBUG_ENDPOINTS=true
ENABLE_SWAGGER_UI=true
ENABLE_REDOC=true

# 테스트 데이터
USE_TEST_DATA=true
MOCK_EXTERNAL_SERVICES=true

# ==============================================================================
# 라이센스 및 규정 준수
# ==============================================================================
# 모델 라이센스 추적
TRACK_MODEL_USAGE=false
COMMERCIAL_USE_APPROVED=false

# 개인정보보호
GDPR_COMPLIANT=true
LOG_USER_QUERIES=false
ANONYMIZE_LOGS=true